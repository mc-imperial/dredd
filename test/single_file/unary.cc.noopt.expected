static int& __dredd_replace_unary_operator_PreInc_int(std::function<int&()> arg, int local_mutation_id) {
  MUTATION_PRELUDE(++arg());
  REPLACE_UNARY_PreDec(arg(), 0);
  REPLACE_UNARY_ARG(arg(), 1);
  return MUTATION_RETURN(++arg());
}

