  unsigned int y = __dredd_replace_expr_unsigned_int([&]() -> unsigned int { return __dredd_replace_expr_int([&]() -> int { return __dredd_replace_unary_operator_Not_int([&]() -> int { return __dredd_replace_expr_int([&]() -> int { return 7; }, 19); }, 25); }, 28); }, 34);
